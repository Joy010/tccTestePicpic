<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- TODO: Update blank fragment layout -->

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="408dp"
        android:layout_height="331dp"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="37dp"
        android:background="@drawable/waves"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="346dp"
        android:layout_height="715dp"
        android:background="@drawable/bg_copo"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/txtDeslize"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Deslize para baixo para sair "
            android:textColor="#6247AA"
            android:textSize="15dp"
            android:textStyle="bold" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="105dp" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Sintomas"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="133dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/cbDorCabe"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Dor De CabeÃ§a "
                    android:textColor="#6247AA"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbFraqueza"
                    android:layout_width="188dp"
                    android:layout_height="wrap_content"
                    android:text="Fraquesa"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbCede"
                    android:layout_width="188dp"
                    android:layout_height="wrap_content"
                    android:text="Cede Excessiva"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/btnAddSint"
                    android:layout_width="75dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/add" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="162dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/cbFadiga"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Fadiga"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbUrinar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Vontade Frequente de Urinar"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="14dp" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Humor"
            android:textColor="#000000"
            android:textSize="30dp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="168dp"
                android:layout_height="185dp"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/cbFeliz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Feliz"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbTriste"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Triste"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbEstresse"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Estersse"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/cbMal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Mal- humor"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <CheckBox
                    android:id="@+id/cbChoro"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Choro"
                    android:textColor="#6247AA"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/btnAddHumor"
                    android:layout_width="103dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/add" />

            </LinearLayout>
        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="135dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/btnCheck"
                android:layout_width="89dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00FFFFFF"
                app:srcCompat="@mipmap/ic_check_foreground" />

            <TextView
                android:id="@+id/txtGlicemia"
                android:layout_width="177dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Bem Estar"
                android:textColor="#6247AA"
                android:textSize="30dp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/btnNext"
                android:layout_width="65dp"
                android:layout_height="55dp"
                android:layout_weight="1"
                android:background="#00FFFFFF"
                app:srcCompat="@mipmap/ic_setalado_foreground" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <ImageButton
            android:id="@+id/btnBolinhasPassar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00FFFFFF"
            app:srcCompat="@drawable/ic_elipsestelaintro3" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>